{
  "name": "node-react-website",
  "version": "1.0.0",
  "description": "Full-stack React website with server side render ",
  "main": "index.js",
  "scripts": {
    "build-server": "webpack --config webpack.server.config.js",
    "prod-server": "nodemon index.js --exec babel-node --presets es2015,stage-2",
    "prod-client-server": "concurrently \"npm run prod-server\""
  },
